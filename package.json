{
  "name": "leveldb-react-semantic-example",
  "version": "0.0.0",
  "description": "an example stack using leveldb, react, and semantic-ui",
  "main": "./index.js",
  "browser": "./src/client.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/holonomy/leveldb-react-semantic-example"
  },
  "author": "",
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/holonomy/leveldb-react-semantic-example/issues"
  },
  "dependencies": {
    "ecstatic": "~0.4.12",
    "less": "~1.5.1",
    "browserify": "~3.14.0",
    "cssmin": "~0.4.1",
    "uglify-js": "~2.4.7",
    "reactify": "~0.7.0",
    "envify": "~0.2.0",
    "react": "~0.8.0",
    "semantic-ui": "git+https://github.com/Semantic-Org/Semantic-UI.git",
    "domready": "~0.2.13",
    "lodash": "~2.4.1",
    "level-live-stream": "~1.4.9",
    "multilevel": "~5.5.0",
    "level": "~0.18.0",
    "shoe": "0.0.15",
    "level-store": "~3.10.1",
    "node-uuid": "~1.4.1",
    "react-backbone-mixin": "git+https://github.com/ahdinosaur/react-backbone-mixin.git",
    "levelsync": "git+https://github.com/ahdinosaur/levelsync.git",
    "jquery": "~2.1.0",
    "backbone": "~1.1.1",
    "express": "~3.4.8"
  },
  "devDependencies": {
    "mocha": "~1.16.0",
    "catw": "~0.2.0",
    "watchify": "~0.6.1",
    "nodemon": "~0.7.10",
    "jshint": "~2.4.0",
    "testling": "~1.5.3",
    "npm-release": "~0.0.4",
    "sinon": "~1.7.3",
    "chai": "~1.8.1",
    "chai-as-promised": "~4.1.0",
    "mocha-as-promised": "~2.0.0"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "scripts": {
    "build-js": "browserify src/client.js | uglifyjs -mc > static/index.js",
    "build-css": "lessc --include-path=src:node_modules/semantic-ui/src src/index.less static/index.css",
    "build": "npm run build-css && npm run build-js",
    "watch-js": "watchify src/client.js -o static/index.js -dv",
    "watch-css": "catw -c 'lessc --include-path=src:node_modules/semantic-ui/src -' 'src/index.less' -o static/index.css -v",
    "watch": "npm run watch-css & npm run watch-js",
    "lint": "jshint src ;true",
    "develop": "npm run watch & nodemon index.js",
    "start": "npm run build && node .",
    "release": "npm-release patch -m \"version++\""
  }
}
